{
    "name": "onFlick",
    "version": "1.0.0",
    "description":
        "Fire an event on a trackpad or mousewheel flick. Scroll values on trackpads can have inertia and take a long time to come back down to 0 - this plugin smooths out those false positive scroll events. [Demo](http://codepen.io/SaFrMo/pen/BRWrbw?editors=1010)",
    "main": "./dist/onFlick.js",
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "build": "babel src --out-dir dist",
        "prepublish": "npm run build"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/SaFrMo/onFlick.git"
    },
    "prettier": {
        "tabWidth": 4,
        "semi": false,
        "singleQuote": true
    },
    "keywords": ["scroll", "flick", "swipe"],
    "author": "",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/SaFrMo/onFlick/issues"
    },
    "homepage": "https://github.com/SaFrMo/onFlick#readme",
    "dependencies": {
        "babel-cli": "^6.26.0",
        "babel-preset-env": "^1.7.0"
    },
    "babel": {
        "presets": ["env"]
    }
}
